{
  "name": "minimal-repro",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pkg-win": "electron-packager ./ ByteTicker --platform=win32 --arch=x64 --out=out --icon=./dist/favicon.ico --overwrite --asar --extra-resource=app",
    "pkg-mac": "electron-packager ./ ByteTicker --platform=darwin --arch=x64 --out=out --icon=./dist/favicon.icns --overwrite --asar --extra-resource=app"
  },
  "repository": "https://github.com/electron/minimal-repro",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^37.2.5",
    "electron-packager": "^17.1.2"
  },
  "build": {
    "extraResources": [
      { "from": "db", "to": "db" },
      { "from": "app/jar", "to": "app" },
      { "from": "app/jre", "to": "app/jre" }
    ]
  }
}
